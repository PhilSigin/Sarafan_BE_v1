{"openapi": "3.1.0", "info": {"title": "NinjaAPI", "version": "1.0.0", "description": ""}, "paths": {"/api/cart/": {"get": {"operationId": "shop_api_get_cart", "summary": "\u041e\u0442\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u0435 \u0441\u043e\u0434\u0435\u0440\u0436\u0438\u043c\u043e\u0433\u043e \u043a\u043e\u0440\u0437\u0438\u043d\u044b", "parameters": [], "responses": {"200": {"description": "OK"}}, "tags": ["Cart"], "security": [{"JWTAuth": []}]}}, "/api/cart/clear/": {"post": {"operationId": "shop_api_clear_cart", "summary": "\u041e\u0447\u0438\u0441\u0442\u043a\u0430 \u043a\u043e\u0440\u0437\u0438\u043d\u044b", "parameters": [], "responses": {"200": {"description": "OK"}}, "tags": ["Cart"], "security": [{"JWTAuth": []}]}}, "/api/cart/remove": {"post": {"operationId": "shop_api_remove_from_cart", "summary": "\u0423\u0434\u0430\u043b\u0435\u043d\u0438\u0435 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0430 \u0438\u0437 \u043a\u043e\u0440\u0437\u0438\u043d\u044b", "parameters": [{"in": "query", "name": "product_id", "schema": {"title": "Product Id", "type": "integer"}, "required": true}], "responses": {"200": {"description": "OK"}}, "tags": ["Cart"], "security": [{"JWTAuth": []}]}}, "/api/cart/add": {"post": {"operationId": "shop_api_add_to_cart", "summary": "\u0414\u043e\u0431\u0430\u0432\u043b\u0435\u043d\u0438\u0435 x \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u043e\u0432 \u0432 \u043a\u043e\u0440\u0437\u0438\u043d\u0443", "parameters": [{"in": "query", "name": "product_id", "schema": {"title": "Product Id", "type": "integer"}, "required": true}, {"in": "query", "name": "quantity", "schema": {"anyOf": [{"exclusiveMinimum": 0, "type": "integer"}, {"type": "null"}], "default": 1, "title": "Quantity"}, "required": false}], "responses": {"200": {"description": "OK"}}, "tags": ["Cart"], "security": [{"JWTAuth": []}]}}, "/api/cart/update": {"post": {"operationId": "shop_api_update_cart_item", "summary": "\u0418\u0437\u043c\u0435\u043d\u0435\u043d\u0438\u0435 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0430 \u0442\u043e\u0432\u0430\u0440\u0430 \u0432 \u043a\u043e\u0440\u0437\u0438\u043d\u0435 (\u0438\u043b\u0438 \u0443\u0434\u0430\u043b\u0435\u043d\u0438\u0435)", "parameters": [{"in": "query", "name": "product_id", "schema": {"title": "Product Id", "type": "integer"}, "required": true}, {"in": "query", "name": "quantity", "schema": {"exclusiveMinimum": -1, "title": "Quantity", "type": "integer"}, "required": true}], "responses": {"200": {"description": "OK"}}, "tags": ["Cart"], "security": [{"JWTAuth": []}]}}, "/api/products": {"get": {"operationId": "shop_api_get_products", "summary": "\u041f\u043e\u043a\u0430\u0437 \u0432\u0441\u0435\u0445 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u043e\u0432", "parameters": [{"in": "query", "name": "limit", "schema": {"default": 100, "minimum": 1, "title": "Limit", "type": "integer"}, "required": false}, {"in": "query", "name": "offset", "schema": {"default": 0, "minimum": 0, "title": "Offset", "type": "integer"}, "required": false}], "responses": {"200": {"description": "OK", "content": {"application/json": {"schema": {"$ref": "#/components/schemas/PagedProductSchema"}}}}}, "description": "<strong>\u042d\u043d\u0434\u043f\u043e\u0438\u043d\u0442 \u0432\u044b\u0432\u043e\u0434\u0430 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u043e\u0432 \u0441 \u043f\u0430\u0433\u0438\u043d\u0430\u0446\u0438\u0435\u0439. </strong>\n              \u041a\u0430\u0436\u0434\u044b\u0439 \u043f\u0440\u043e\u0434\u0443\u043a\u0442 \u0432 \u0432\u044b\u0432\u043e\u0434\u0435 \u0434\u043e\u043b\u0436\u0435\u043d \u0438\u043c\u0435\u0442\u044c \u043f\u043e\u043b\u044f: \u043d\u0430\u0438\u043c\u0435\u043d\u043e\u0432\u0430\u043d\u0438\u0435, slug, \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f, \u043f\u043e\u0434\u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f, \n              \u0446\u0435\u043d\u0430, \u0441\u043f\u0438\u0441\u043e\u043a \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u0439. \u041d\u0435 \u0432\u044b\u043f\u043e\u043b\u043d\u0435\u043d \u043f\u043e\u043a\u0430\u0437 \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u044b\u0445 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u043e\u0432.", "tags": ["Products"]}}, "/api/categories": {"get": {"operationId": "shop_api_get_categories", "summary": "\u041a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438 \u0432 \u0434\u0440\u0435\u0432\u043e\u0432\u0438\u0434\u043d\u043e\u043c \u0432\u0438\u0434\u0435", "parameters": [{"in": "query", "name": "limit", "schema": {"default": 100, "minimum": 1, "title": "Limit", "type": "integer"}, "required": false}, {"in": "query", "name": "offset", "schema": {"default": 0, "minimum": 0, "title": "Offset", "type": "integer"}, "required": false}], "responses": {"200": {"description": "OK", "content": {"application/json": {"schema": {"$ref": "#/components/schemas/PagedCategorySchema"}}}}}, "tags": ["Categories"]}}, "/api/categories/": {"get": {"operationId": "shop_api_get_categories_flat", "summary": "\u041a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438 \u0432 \u043f\u043b\u043e\u0441\u043a\u043e\u043c \u0432\u0438\u0434\u0435", "parameters": [], "responses": {"200": {"description": "OK", "content": {"application/json": {"schema": {"items": {"$ref": "#/components/schemas/CategoryFlatSchema"}, "title": "Response", "type": "array"}}}}}, "tags": ["Categories"]}}, "/api/protected_data": {"get": {"operationId": "shop_api_protected_data", "summary": "\u041f\u0440\u043e\u0432\u0435\u0440\u043a\u0430 \u043d\u0430 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u043e\u0441\u0442\u044c \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438 \u043f\u043e \u0442\u043e\u043a\u0435\u043d\u0443", "parameters": [], "responses": {"200": {"description": "OK"}}, "tags": ["Test"], "security": [{"JWTAuth": []}]}}, "/api/protected_data_test": {"get": {"operationId": "shop_api_protected_data_test", "summary": "\u041f\u0440\u043e\u0432\u0435\u0440\u043a\u0430 \u0442\u043e\u043a\u0435\u043d\u0430", "parameters": [], "responses": {"200": {"description": "OK"}}, "tags": ["Test"], "security": [{"JWTAuth": []}]}}, "/api/token/pair": {"post": {"operationId": "token_obtain_pair", "summary": "Obtain Token", "parameters": [], "responses": {"200": {"description": "OK", "content": {"application/json": {"schema": {"$ref": "#/components/schemas/TokenObtainPairOutputSchema"}}}}}, "tags": ["Auth"], "requestBody": {"content": {"application/json": {"schema": {"$ref": "#/components/schemas/TokenObtainPairInputSchema"}}}, "required": true}}}, "/api/token/refresh": {"post": {"operationId": "token_refresh", "summary": "Refresh Token", "parameters": [], "responses": {"200": {"description": "OK", "content": {"application/json": {"schema": {"$ref": "#/components/schemas/TokenRefreshOutputSchema"}}}}}, "tags": ["Auth"], "requestBody": {"content": {"application/json": {"schema": {"$ref": "#/components/schemas/TokenRefreshInputSchema"}}}, "required": true}}}}, "components": {"schemas": {"Input": {"properties": {"limit": {"default": 100, "minimum": 1, "title": "Limit", "type": "integer"}, "offset": {"default": 0, "minimum": 0, "title": "Offset", "type": "integer"}}, "title": "Input", "type": "object"}, "CategoryAncestorListSchema": {"description": "SHOWN AS A LIST IN PRODUCTS LIST ", "properties": {"id": {"title": "Id", "type": "integer"}, "cat_name": {"title": "Cat Name", "type": "string"}, "cat_slug": {"title": "Cat Slug", "type": "string"}, "cat_img": {"title": "Cat Img", "type": "string"}, "parent": {"anyOf": [{"type": "integer"}, {"type": "null"}], "title": "Parent"}}, "required": ["id", "cat_name", "cat_slug", "cat_img", "parent"], "title": "CategoryAncestorListSchema", "type": "object"}, "PagedProductSchema": {"properties": {"items": {"items": {"$ref": "#/components/schemas/ProductSchema"}, "title": "Items", "type": "array"}, "count": {"title": "Count", "type": "integer"}}, "required": ["items", "count"], "title": "PagedProductSchema", "type": "object"}, "Pr